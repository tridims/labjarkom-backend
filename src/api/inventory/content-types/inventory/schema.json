{
  "kind": "collectionType",
  "collectionName": "inventories",
  "info": {
    "singularName": "inventory",
    "pluralName": "inventories",
    "displayName": "Inventory"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "uid",
      "targetField": "name",
      "required": true,
      "maxLength": 8,
      "minLength": 8
    },
    "quantity": {
      "type": "integer",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "available": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
