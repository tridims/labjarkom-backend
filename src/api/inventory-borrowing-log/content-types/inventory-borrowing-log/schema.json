{
  "kind": "collectionType",
  "collectionName": "inventory_borrowing_logs",
  "info": {
    "singularName": "inventory-borrowing-log",
    "pluralName": "inventory-borrowing-logs",
    "displayName": "Inventory Borrowing Log"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "idPeminjaman": {
      "type": "uid",
      "required": true
    },
    "User": {
      "displayName": "People Data",
      "type": "component",
      "repeatable": false,
      "component": "people.people-data",
      "required": true
    },
    "dateOfReturn": {
      "type": "date",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "not yet borrowed",
        "being borrowed",
        "missing",
        "returned"
      ],
      "required": true,
      "default": "not yet borrowed"
    }
  }
}
